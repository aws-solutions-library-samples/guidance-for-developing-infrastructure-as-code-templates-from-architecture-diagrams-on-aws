{
  "name": "a2cai",
  "version": "0.1.0",
  "bin": {
    "app": "bin/a2cai.js"
  },
  "scripts": {
    "build": "tsc && tsc --build --clean && npm run lint",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "npx aws-cdk@2.219.0",
    "validate": "npx ts-node ./scripts/package-checksum-validator.ts",
    "validate:fix": "ts-node ./scripts/package-checksum-generator.ts",
    "audit": "concurrently 'npm:audit:*(!fix)'",
    "audit:deps:nodejs": "npx better-npm-audit@3.7.3 audit --level=high --registry='https://registry.npmjs.org/'",
    "audit:deps:python": "./scripts/check-deps-python.sh",
    "audit:scan:security": "./scripts/check-code-scan-security.sh",
    "audit:license": "npm run license",
    "audit:fix:license": "npm run license -- -u",
    "license": "run-script-os --",
    "license:default": "./scripts/check-licenses.sh",
    "license:macos": "./scripts/check-licenses-locally.sh",
    "lint": "npx eslint . --ext .ts --max-warnings 0",
    "lint:fix": "npx eslint . --ext .ts --fix",
    "commitlint": "commitlint --edit"
  },
  "config": {
    "applicationName": "A2C-AI",
    "cdkQualifier": "a2c"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.2",
    "@types/prettier": "2.7.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/source-map-support": "0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "aws-cdk": "^2.219.0",
    "concurrently": "9.2.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "run-script-os": "1.1.6",
    "ts-jest": "^29.4.4",
    "ts-node": "10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@cloudcomponents/cdk-pull-request-approval-rule": "2.4.0",
    "@cloudcomponents/cdk-pull-request-check": "2.4.0",
    "@cloudscape-design/chat-components": "^1.0.66",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/visitor-keys": "8.45.0",
    "aws-cdk-lib": "^2.219.0",
    "cdk-monitoring-constructs": "9.15.2",
    "cdk-nag": "2.37.40",
    "constructs": "10.4.2",
    "react-router-dom": "^7.9.3",
    "react-use": "^17.6.0",
    "source-map-support": "^0.5.21",
    "uuid": "13.0.0"
  },
  "overrides": {
    "http-cache-semantics": "4.1.1",
    "semver": "7.5.3",
    "@babel/traverse": "7.23.2",
    "tmp": "0.2.5"
  }
}
